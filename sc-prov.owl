    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Action</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Action is the scope of the expected activity performed by agents that correspond to sc-prov:SocialActorSpec.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Action</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Action</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Condition</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">An sc-prov:Condition defines a state of the world described by a logical state- ment that is used to define a desired state of a particular aspect of a successful step.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Condition</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Condition</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:EvaluationContext</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">A concept used to connect the evaluation result of a particular sc-prov:Condition (C1) with a single executed p-plan:Activity corresponding to the step associated with C1.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:EvaluationContext</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Evaluation Context</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Imposition</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Imposition is the subjection of p-plan:Step to the constraints described by sc- prov:Condition.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Imposition</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Imposition</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Incentive</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Incentive is a kind of p-plan:Variable and represents a thing (e.g. monetary value, increased knowledge, etc.) that would be realised by a worker following successful completion of a task.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Incentive</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Incentive</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Input</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Input describes a specification context in which a p-plan:Variable is expected to be taken as an input for a p-plan:Step.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Input</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Input</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Output</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Output describes a specification context in which a p-plan:Variable is expected
to be produced as an output of a p-plan:Step.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Output</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Output</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:ParameterCollection</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">A kind of prov:Collection which is used to encapsulate the in-
stances of prov:Entity that were used as parameters to evaluate a particular sc-prov:Condition.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:ParameterCollection</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Parameter Collection</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:Purpose</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Purpose is the expected scope of influence of a p-plan:Variable or an sc- prov:Condition on a p-plan:Step.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:Purpose</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Purpose</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:RoleSpec</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">A role specification is the function of a variable, a social actor, or a condition in the context of a particular step.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:RoleSpec</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Role Specification</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:SocialActorSpec</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">An sc-prov:SocialActor is a kind of p-plan:Variable and represents a desired specification of an agent who should be associated with a p-plan:Step in order to perform a non- administrative function in a social computation process.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:SocialActorSpec</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Social Actor Specification</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:correspondsToActor</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">A relation that links a prov:Agent to its expected description captured by sc-prov-o:SocialActorSpec.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:correspondsToActor</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">correspondsToActor</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:hadCondition</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">A relation that links a specific sc-prov-o:evaluationContext to the sc-prov-o:Condition for which the evaluation was sought for.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:hadCondition</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">hadCondition</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:hadEvaluationSubject</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">A relation describing that a particular p-plan:Activity captured as
part of the sc-prov-o:EvaluationContext is the activity for which the condition was evaluated.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <AbbreviatedIRI>SC-PROV-0:hadEvaluationSubject</AbbreviatedIRI>
        <Literal datatypeIRI="&xsd;string">hadActivity</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <AbbreviatedIRI>SC-PROV-0:hadParameterCollection</AbbreviatedIRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A relation that is used to associate an sc-prov:ParameterCollection with an sc-prov:EvaluationContext</Literal>
    </AnnotationAssertion>
